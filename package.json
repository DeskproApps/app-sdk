{
  "name": "@deskpro/app-sdk",
  "version": "0.1.21",
  "description": "Deskpro Apps SDK",
  "private": false,
  "license": "proprietary",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "dist",
    "esm",
    "cjs",
    "README.md"
  ],
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "@deskpro/deskpro-ui": "0.1.14",
    "handlebars": "4.7.7",
    "penpal": "6.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.23.0",
    "regenerator-runtime": "0.13.9",
    "styled-components": "5.3.3",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.12",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.19",
    "@typescript/lib-dom": "npm:@types/web",
    "fast-glob": "3.2.7",
    "jest": "27.4.4",
    "node-sass": "7.0.0",
    "rollup": "2.61.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-scss": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript": "1.0.1",
    "scss-reset": "1.2.2",
    "terser": "5.10.0",
    "typescript": "4.5.5"
  },
  "scripts": {
    "start": "yarn start:storybook",
    "build": "yarn build:esm && yarn build:cjs && yarn build:dist",
    "build:dist": "rimraf ./dist && rollup --config rollup.config.js && terser dist/deskpro.js -o dist/deskpro.min.js -m --source-map",
    "build:esm": "rimraf ./esm && tsc",
    "build:cjs": "rimraf ./cjs && babel ./src --out-dir ./cjs --extensions .ts,.tsx --ignore \"**/*.stories.tsx\" --plugins @babel/plugin-transform-modules-commonjs",
    "watch:dist": "rimraf ./dist && rollup --config rollup.config.js --watch",
    "start:storybook": "start-storybook -p 6008",
    "test": "jest"
  }
}
